<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema id="org.gnome.shell.extensions.switcher" path="/org/gnome/shell/extensions/switcher/">
        <key type="as" name="show-switcher">
            <default><![CDATA[['<Super>w']]]></default>
            <summary>Global key combination to show the switcher.</summary>
        </key>
        <key type="b" name="activate-immediately">
            <default>false</default>
            <summary>Whether to activate immediately when there is just one result.</summary>
        </key>
        <key type="u" name="matching">
            <default>0</default>
            <summary>Pattern matching algorithm: 0=Substring, 1=Fuzzy</summary>
        </key>
        <key type="u" name="ordering">
            <default>0</default>
            <summary>Ordering criteria: 0=Last focused, 1=Relevancy</summary>
        </key>
        <key type="u" name="font-size">
            <default>32</default>
            <summary>Font size</summary>
        </key>
        <key type="u" name="icon-size">
            <default>32</default>
            <summary>Icon size</summary>
        </key>
        <key type="u" name="max-width-percentage">
            <default>95</default>
            <summary>Maximum percentage of screen width to take</summary>
        </key>
        <key type="u" name="activate-by-key">
            <default>0</default>
            <summary>Activate by pressing the index in the list: 0=No, 1=Function keys, 2=Number keys</summary>
        </key>
        <key type="u" name="activate-after-ms">
            <default>0</default>
            <summary>Activate "immediately" after this many milliseconds</summary>
        </key>
        <key type="b" name="workspace-indicator">
            <default>false</default>
            <summary>Whether to show a workspace indicator for each app</summary>
        </key>
        <key type="b" name="only-current-workspace">
            <default>false</default>
            <summary>Whether to show just the apps in the current workspace</summary>
        </key>
    </schema>
</schemalist>
